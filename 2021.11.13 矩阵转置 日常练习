#include <stdio.h> 
int main() 
{ 
	int x,y; 
	int m=3,n=3; //m*n 矩阵 
	int a[3][3]; 
	int b[3][3]; 
	
	for(x = 0; x < 3; x++)
	{
		for(y = 0; y < 3;y++)
			scanf("%d", &a[x][y]);
	}
	for(x=0;x<n;x++) 
		for(y=0;y<m;y++) 
			b[y][x]=a[x][y]; 

	for(x=0;x<n;x++) 
	{ 
		for(y=0;y<m;y++) 
		 	printf("   %d",b[x][y]); 
		printf("\n"); 
	
	} 	 
	return 0; 
} 
